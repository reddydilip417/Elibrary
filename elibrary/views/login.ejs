     <div id="loginModal" class="modal fade" role="dialog">
        <div class="modal-dialog" style="width:610px">
        <!-- Modal content-->
        <div class="modal-content">
            <div class="modal-header">
                <button type="button" class="close" data-dismiss="modal">&times;</button>
                <h4 class="modal-title">Admin Login </h4>
            </div>
            <div class="modal-body">

               <form class="form-inline" method="put" action="/upload">
                 
                 <div class="form-group">
                 
                     <input type="text" required pattern="admin" class="form-control input-sm" style="border-radius:0;" placeholder="Username">
                     <input type="password" required pattern="admin" class="form-control input-sm" style="border-radius:0" placeholder="Password">
                    
                     <button class="btn btn-info btn-default btn-sm" style="padding:4px" type="submit">Sign in</button>
                     <button type="button" class="btn btn-default btn-sm" data-dismiss="modal">Cancel</button>
                     <br\>
                             
                 </div>
                 
                </form>

            </div>
        </div>
        </div>
