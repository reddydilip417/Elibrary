            </div>
            <div class="modal-body">

              <form class="form-inline" method="post" action="/">
              <div class="form-group">
            <div class="has-icon-left form-control-email">
              <label class="sr-only" for="inputEmail">Email</label>
              <input required name="email" type="email" class="input-sm form-control form-control-lg" id="inputEmail" placeholder="Email address" autocomplete="off">
            </div>
            <div class="has-icon-left form-control-password">
              <label class="sr-only" for="inputPassword">password</label>
              <input required name="password" type="password" class="form-control form-control-lg input-sm" id="inputPassword" placeholder="Enter password" autocomplete="off">
            </div>
              </div>
            <!-- Buttons
            ================================================== -->

            <button type="submit" class="btn btn-primary btn-sm btn-block" style="width:70px">Log in!</button>
            <button type="button" class="btn btn-default btn-sm" data-dismiss="modal">Cancel</button>
            <p>Don't have an Account? Please <a href="signup">SignUp</a> </p>  
            
          </form>

